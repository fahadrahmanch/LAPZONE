  <script>
    document.addEventListener('DOMContentLoaded', () => {
  document.querySelectorAll('.wishlist-link').forEach(link => {
    const heartIcon = link.querySelector('svg');
    const productId = link.dataset.productId;
    const variantId = link.dataset.variantId;
    const isInWishlist = link.dataset.inWishlist === 'true';

    // Set initial color based on whether it's in the wishlist
    heartIcon.setAttribute('fill', isInWishlist ? 'red' : 'none');

    link.addEventListener('click', async function (event) {
      event.preventDefault();

      const isCurrentlyInWishlist = heartIcon.getAttribute('fill') === 'red';
      const newState = !isCurrentlyInWishlist;
      heartIcon.setAttribute('fill', newState ? 'red' : 'none');

      await toggleWishlist(productId, newState, variantId);
    });
  });
});

async function toggleWishlist(productId, isAdding, variantId) {
  try {
    const url = isAdding ? '/wishlist/add' : '/wishlist/remove';
    const response = await fetch(url, {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
      },
      body: JSON.stringify({ productId, variantId }),
    });

    const data = await response.json();

    if (data.success) {

    Toastify({
        text: `<i class="fas ${isAdding ? 'fa-heart' : 'fa-heart-broken'}"></i> <b>${isAdding ? "Added to Wishlist!" : "Removed from Wishlist!"}</b>`,
        duration: 1500,
        close: false, 
        gravity: "top",
        position: "right",
        stopOnFocus: true,
        escapeMarkup: false, 
        style: {
            background: isAdding ? "#28a745" : "#dc3545",  
            color: "#fff",
            fontSize: "14px",
            borderRadius: "6px",
            padding: "10px 18px",
            boxShadow: "0px 4px 12px rgba(0, 0, 0, 0.15)",
            textAlign: "center",
            display: "flex",
            alignItems: "center",
            gap: "10px"
        }
    }).showToast();
}
else {
    // alert("jh")  
    Toastify({
        text: `<i class="fas fa-times-circle"></i> <b>${data.message || "Something went wrong!"}</b>`,
        duration: 4000,
        close: true,
        gravity: "top",
        position: "right",
        stopOnFocus: true,
        escapeMarkup: false,
        style: {
            background: "#c82333",  // Error red
            color: "#fff",
            fontSize: "14px",
            borderRadius: "6px",
            padding: "10px 18px",
            boxShadow: "0px 4px 12px rgba(0, 0, 0, 0.15)",
            textAlign: "center",
            display: "flex",
            alignItems: "center",
            gap: "10px"
        }
    }).showToast();
}

  } catch (error) {
    Toastify({
        text: `<i class="fas fa-exclamation-triangle"></i> <b>An error occurred while updating the wishlist. Please try again later.</b>`,
        duration: 4000,
        close: true,
        gravity: "top",
        position: "right",
        stopOnFocus: true,
        escapeMarkup: false,
        style: {
            background: "#ff5733",  // Reddish-orange for warnings
            color: "#fff",
            fontSize: "14px",
            borderRadius: "6px",
            padding: "10px 18px",
            boxShadow: "0px 4px 12px rgba(0, 0, 0, 0.15)",
            textAlign: "center",
            display: "flex",
            alignItems: "center",
            gap: "10px"
        }
    }).showToast();
    console.error(error);
  }
}
