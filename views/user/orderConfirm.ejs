                    </tr>
                    <tr>
                        <!-- <td class="p-2 border border-gray-300 font-bold" colspan="3">
                         Shipping
                        </td> -->
                        <td class="p-2 border border-gray-300 font-bold" colspan="3">
                         Delivary Charge
                        </td>
                        <td class="p-2 border border-gray-300">
                            <%=orderDetails.DeliveryCharge%>   
                        </td>
                       </tr>
                    <tr>
                     <td class="p-2 border border-gray-300 font-bold" colspan="3">
                      Total
                     </td>
                     <td class="p-2 border border-gray-300 font-bold">
                        <%=orderDetails.finalAmount%>
                     </td>
                    </tr>
                   </tfoot>
                  </table>
