    <!-- ...:::: Start Customer Login Section :::... -->
    <div class="customer-login ">
        <div class="container ">
            <div class="row justify-content-center align-items-center vh-1">
                <!--register area start-->
                <div class="col-lg-6 col-md-6">
                    <div class="account_form register" data-aos="fade-up" data-aos-delay="200">
                        <h3>Create an account</h3>
                        <form action="/signup" method="post" id="signupForm" target="_self">
                            <!-- Registration Form -->
                            <div class="form-group mb-3">
                               <small class="text-danger"><%= message %></small> <br>
                                <label for="name" class="mb-2">Your name <span class="text-danger">*</span></label>
                                <input type="text" name="name" class="form-control" id="name" placeholder="Enter your name" oninput="validateUsername()">
                                <small id="name_error" class="text-danger"></small>
                            </div>
                            
                            <div class="form-group mb-3">
                                <label for="email" class="mb-2">Enter Email <span class="text-danger">*</span></label>
                                <input type="text" name="email" class="form-control" id="email" placeholder="Enter your email" oninput=" validateEmail()">
                                <small id="email_error" class="text-danger"></small>
                            </div>
                        
                            <div class="form-group mb-3">
                                <label for="phone" class="mb-2">Phone number <span class="text-danger">*</span></label>
                                <input type="tel" name="phone" class="form-control" id="phone" placeholder="Enter your phone number" oninput="validateMobile()">
                                <small id="phone_error" class="text-danger"></small>
                            </div>
                        
                            <div class="form-group mb-3">
                                <label for="password" class="mb-2">Password <span class="text-danger">*</span></label>
                                <input type="password" name="password" class="form-control" id="password" placeholder="Enter your password"  oninput="validatePassword()">
                                <small id="password_error" class="text-danger"></small>
                            </div>
                        
                            <div class="form-group mb-4">
                                <label for="confirm-password" class="mb-2">Confirm Password <span class="text-danger">*</span></label>
                                <input type="password" class="form-control" id="confirmpassword" placeholder="Confirm your password" oninput="validateConfirmPassword()">
                                <small id="confirmpassword_error" class="text-danger"></small>
                            </div>
                        
                            <!-- Register Button -->
                            <div class="form-group mb-3">
                                <button class="btn btn-dark btn-block btn-sm" type="submit">Register</button>
                            </div>
                        
                            <!-- Google Authentication Button -->
                            <div class="form-group text-center mt-4">
                                <p>or sign in with</p>
                                <a class="btn btn-danger btn-block btn-sm" href="/auth/google">Sign in with Google</a> <!-- Smaller Google button -->
                            </div>
                            <a href="/login">Already Have an Account?</a>
                        </form>
                       
                        <script src="/js_folder/validation.js"></script>
                        
                    </div>
                </div>
                <!--register area end-->
            </div>
        </div>
    </div> <!-- ...:::: End Customer Login Section :::... -->
