<script>
    async function toggleWishlist(variantId, productId) {
      const confirmDelete = await Swal.fire({
        title: "Are you sure?",
        text: "Do you really want to remove this item from your wishlist?",
        icon: "warning",
        showCancelButton: true,
        confirmButtonColor: "#d33",
        cancelButtonColor: "#3085d6",
        confirmButtonText: "Yes",
        cancelButtonText: "No",
      });
  
      if (!confirmDelete.isConfirmed) {
        return; // Stop execution if the user clicks "No"
      }
  
      try {
        const url = '/wishlist/remove';
        const response = await fetch(url, {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify({ productId, variantId }),
        });
  
        const data = await response.json();
        if (data.success) {
          await Swal.fire("Removed!", "Item has been removed from your wishlist.", "success");
          window.location.reload();
        } else {
          Swal.fire("Error", data.message, "error");
        }
      } catch (error) {
        Swal.fire("Error", "An error occurred while updating the wishlist.", "error");
        console.error(error);
      }
    }
  </script>

