    <script>
        async function addToCart(product,variant,qty) {
      try {
        console.log("id",product,variant)
        //   const variantSelected = document.getElementById('variantSelect').value;
        //   console.log("VARIAN",variantSelected)
          const response = await fetch('/cart/add', {
              method: 'POST',
              headers: {
                  'Content-Type': 'application/json'
              },
              body: JSON.stringify({
                  product: product,
                  variant: variant,
                  qty
              })
          });
  
          if (response.ok) {
              const result = await response.json();
              console.log('Item added to cart:', result);
  
              Swal.fire({
                  icon: 'success',
                  title: 'Success',
                  text: result.message || 'Item successfully added to the cart!',
                  confirmButtonText: 'OK'
              });
          } else {
              const error = await response.json();
              console.error('Error adding item:', error);
  
      
              Swal.fire({
                  icon: 'error',
                  title: 'Error',
                  text: error.message || 'Failed to add item to the cart. Please try again.',
                  confirmButtonText: 'OK'
              });
          }
      } catch (err) {
          console.error('Network error:', err);
  
        
          Swal.fire({
              icon: 'warning',
              title: 'Network Error',
              text: 'An error occurred. Please check your connection and try again.',
              confirmButtonText: 'OK'
          });
      }
  }
  
