              <div class="password-modal-overlay" id="password-modal-overlay">
                <div class="password-modal">
                  <form id="changepassword">  
                    <h2>Change Password</h2>
                    <input
                      type="password"
                      name="currentpassword"
                      placeholder="Enter your current password"
                      id="oldpass"
                      oninput="pass()"
                    />
                    <span class="text-danger" id="oldpass_error"></span>
                    <input
                      type="password"
                      name="newpassword"
                      placeholder="Enter new password"
                      id="password"
                      oninput="validatenewpass()"
                    />
                    <span id="password_error"></span>
                    <input
                      type="password"
                      name="confirmpassword"
                      placeholder="Confirm new password"
                      id="confirmpassword"
                      oninput="validateconfirmpass()"
                    />
                    <span id="confirmpassword_error"></span>
                    <div class="password-button-row">
                      <button
                        type="submit"
                        class="password-save-button"
                        id="submit"
                      >
                        Save Changes
                      </button>
                      <button
                        type="button"
                        class="password-cancel-button"
                        onclick="closePasswordModal()"
                      >
                        Cancel
                      </button>
                    </div>
                  </form>
                </div>
              </div>
              <script>
                // Get the form element
                const changepassword =
                  document.getElementById("changepassword");

                // Attach a single submit event listener
                if (changepassword) {
                  changepassword.addEventListener("submit", async (event) => {
                    // Prevent form submission
                    event.preventDefault();

                    // Perform validations
                    const isPassValid = pass();
                    const isNewPassValid = validatenewpass();
                    const isConfirmPassValid = validateconfirmpass();

                    if (
                      !isPassValid ||
                      !isNewPassValid ||
                      !isConfirmPassValid
                    ) {
                      // If validation fails, stop execution
                      return;
                    }

                    // Get the form data
                    const currentpassword =
                      document.getElementById("oldpass").value;
                    const newpassword =
                      document.getElementById("password").value;

                    try {
                      // Make the fetch request
                      const response = await fetch(
                        `/change-password/<%=users._id%>`,
                        {
                          method: "POST",
                          headers: {
                            "Content-Type": "application/json",
                          },
                          body: JSON.stringify({
                            currentpassword,
                            newpassword,
                          }),
                        }
                      );

                      const data = await response.json();

                      // Handle the response
                      if (data.success) {
                        closePasswordModal();
                        Swal.fire({
                          title: "Success",
                          text: "Password changed successfully!",
                          icon: "success",
                        });
                      } else {
                        await closePasswordModal();
                        await Swal.fire({
                          title: "Error",
                          text: data.message || "Failed to change password.",
                          icon: "error",
                          confirmButtonText: "OK",
                        });
                        await openPasswordModal();
                      }
                    } catch (error) {
                      console.error("Error:", error);
                      Swal.fire({
                        title: "An error occurred",
                        text: "Something went wrong. Please try again later.",
                        icon: "error",
                        confirmButtonText: "OK",
                      });
                    }
                  });
                }

                // Validation function for the old password
                function pass() {
                  let isValid = true;
                  const password = document.getElementById("oldpass");
                  const passwordError =
                    document.getElementById("oldpass_error");

                  passwordError.textContent = "";

                  if (password.value.trim() === "") {
                    passwordError.textContent = "Old password is required.";
                    isValid = false;
                  } else if (password.value.trim().length < 6) {
                    passwordError.textContent =
                      "Old password must be at least 6 characters.";
                    isValid = false;
                  }

                  return isValid;
                }

                // Validation function for the new password
                function validatenewpass() {
                  let isValid = true;
                  const password = document.getElementById("password");
                  const passwordError =
                    document.getElementById("password_error");

                  passwordError.textContent = "";

                  if (password.value.trim() === "") {
                    passwordError.textContent = "New password is required.";
                    isValid = false;
                  } else if (password.value.trim().length < 6) {
                    passwordError.textContent =
                      "New password must be at least 6 characters.";
                    isValid = false;
                  }

                  return isValid;
                }

                // Validation function for confirm password
                function validateconfirmpass() {
                  let isValid = true;
                  const password = document.getElementById("password");
                  const confirmpassword =
                    document.getElementById("confirmpassword");
                  const confirmpasswordError = document.getElementById(
                    "confirmpassword_error"
                  );

                  confirmpasswordError.textContent = "";

                  if (confirmpassword.value.trim() === "") {
                    confirmpasswordError.textContent =
                      "Confirm password is required.";
                    isValid = false;
                  } else if (password.value !== confirmpassword.value) {
                    confirmpasswordError.textContent =
                      "Passwords do not match.";
                    isValid = false;
                  }

                  return isValid;
                }
                // Open the Change Password modal
                function openPasswordModal() {
                  document.getElementById(
                    "password-modal-overlay"
                  ).style.display = "flex";
                }

                // Close the Change Password modal
                function closePasswordModal() {
                  document.getElementById(
                    "password-modal-overlay"
                  ).style.display = "none";
                }
              </script>

