          <div class="col-sm-12 col-md-9 col-lg-9">
            <!-- Tab panes -->
            <div
              class="tab-content dashboard_content"
              data-aos="fade-up"
              data-aos-delay="200"
            >
              <div class="tab-pane fade show active" id="dashboard">
                <div class="profile-card-container">
                  <div class="profile-card">
                    <img
                      src="/pictures/6f789671-5fda-4caf-87d4-99787a61.jpg"
                      alt="User Image"
                    />
                    <h2><%=users.name%></h2>
                    <p><span>Email:</span> <%=users.email%></p>
                    <p><span>Phone:</span> <%=users.phone%></p>
                    <div class="button-row">
                      <button class="edit-button" onclick="openCustomModal()">
                        Edit Profile
                      </button>
                      <button
                        class="password-button"
                        onclick="openPasswordModal()"
                      >
                        Change Password
                      </button>
                    </div>
                  </div>
                </div>

                <style>
                  /* Centering the profile card */
                  .profile-card-container {
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    height: 50vh;
                  }

                  /* Profile card styling */
                  .profile-card {
                    background: #fff;
                    width: 800px; /* Increased width for double size */
                    border-radius: 10px;
                    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
                    padding: 20px;
                    text-align: center;
                    background-color: #eadec9;
                  }

                  /* Profile image styling */
                  .profile-card img {
                    width: 150px;
                    height: 150px;
                    border-radius: 50%;
                    object-fit: cover;
                    object-position: center;
                    margin-bottom: 15px;
                  }

                  /* Title styling */
                  .profile-card h2 {
                    font-size: 28px; /* Slightly larger for better fit with wider card */
                    font-weight: bold;
                    margin-bottom: 10px;
                  }

                  /* Profile information */
                  .profile-card p {
                    font-size: 18px;
                    margin: 10px 0;
                    color: #555;
                  }

                  .profile-card p span {
                    font-weight: bold;
                  }

                  /* Buttons row styling */
                  .button-row {
                    display: flex;
                    justify-content: space-around; /* Space between buttons */
                    margin-top: 20px;
                  }

                  /* Button styling */
                  .profile-card button {
                    width: 45%; /* Adjusted for two buttons in one row */
                    padding: 10px;
                    font-size: 16px;
                    border: none;
                    border-radius: 5px;
                    cursor: pointer;
                  }

                  .edit-button {
                    background-color: #007bff;
                    color: white;
                  }

                  .edit-button:hover {
                    background-color: #0056b3;
                  }

                  .password-button {
                    background-color: #28a745;
                    color: white;
                  }

                  .password-button:hover {
                    background-color: #1e7e34;
                  }
                </style>
              </div>

              <!-- edit user profile -->
              <style>
                /* Overlay for the modal */
                .custom-modal-overlay {
                  display: none;
                  position: fixed;
                  top: 0;
                  left: 0;
                  width: 100%;
                  height: 100%;
                  background: rgba(
                    0,
                    0,
                    0,
                    0.6
                  ); /* Transparent dark background */
                  z-index: 1000;
                  justify-content: center;
                  align-items: center;
                }

                /* Modal container with custom background */
                .custom-modal {
                  width: 600px;
                  border-radius: 10px;
                  padding: 20px;
                  z-index: 1001;
                  animation: custom-fadeIn 0.3s ease-in-out;
                  background-color: #eadec9; /* Custom background color */
                  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
                }

                /* Form styling inside modal */
                .custom-modal input[type="text"],
                .custom-modal input[type="email"],
                .custom-modal input[type="tel"],
                .custom-modal input[type="file"] {
                  width: 100%;
                  padding: 10px;
                  margin: 10px 0;
                  border: 1px solid #ccc;
                  border-radius: 5px;
                  font-size: 16px;
                }

                .custom-modal h2 {
                  font-size: 24px;
                  margin-bottom: 20px;
                }

                .custom-button-row {
                  display: flex;
                  justify-content: space-between;
                  margin-top: 20px;
                }

                .custom-modal button {
