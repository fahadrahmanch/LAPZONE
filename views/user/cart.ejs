    function updateCartTotal(){
        let newSubtotal=0;
        document.querySelectorAll('.product_total').forEach((item)=>{
            const priceText=item.innerHTML.replace("₹",'').trim();
            const price=parseFloat(priceText)||0
            newSubtotal+=price
            console.log('newSubtotal',newSubtotal)
        });
        const Subtotal=document.getElementById('subtotalAmount')
        const totalAmount=document.getElementById('totalAmount')
        if(!Subtotal||!totalAmount){
            console.error("Error: subtotalAmount or totalAmount not found in the DOM!");
            return
        }
            Subtotal.innerHTML = `₹ ${newSubtotal.toFixed(2)}`;
            totalAmount.innerHTML= `₹ ${newSubtotal.toFixed(2)}`;
   
    }
</script>
    <script>
     async function updateQuantity(productId,  variantId,quantity) {
        event.preventDefault()
       fetch('/cart/quantity/update', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ productId, quantity ,variantId}),
    })
        .then((response) => response.json())
        .then(async(data) => {
            if (data.success) {
           

              await Swal.fire({
                    icon: 'success',
                    title: 'Quantity Updated',
                    text: `The quantity was updated successfully!`,
                    timer: 1500,
                    showConfirmButton: false,
                });
            //    window.location.reload()
            const prtotal = document.querySelector(`td[data-id="${productId}"]`);
if (prtotal) {
    const offerPrice = parseFloat(prtotal.getAttribute('data-offerPrice')) || 0;
    const salePrice = parseFloat(prtotal.getAttribute('data-salePrice')) || 0;
    const finalPrice = quantity * (offerPrice || salePrice);
    
    prtotal.innerHTML = `₹ ${finalPrice.toFixed(2)}`;
}

updateCartTotal();
            } else {
                Swal.fire({
                    icon: 'error',
                    title: 'Update Failed',
                    text: data.message || 'Could not update the quantity.',
                });
            }
        })
        .catch((error) => {
            console.error('Error:', error);
            alert(error);

            Swal.fire({
                icon: 'error',
                title: 'Error',
                text: 'An unexpected error occurred. Please try again later.',
            });
        });
}

    </script>

    <script>
  async function dltCartItem(productId, variantId) {
    try {
        event.preventDefault();
        const confirmation = await Swal.fire({
            title: 'Are you sure?',
            text: "Do you really want to delete this product from your cart?",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#d33',
            cancelButtonColor: '#3085d6',
            confirmButtonText: 'Yes, delete it!',
            cancelButtonText: 'Cancel'
        });

       
        if (confirmation.isConfirmed) {
            console.log("Sending request to delete product with ID:", productId);

            const response = await fetch('/cart/product/delete', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ productId, variantId })
            });

            const result = await response.json();
            console.log("Server response:", result);

            if (response.status) {
               await Swal.fire({
                    icon: 'success',
                    title: 'Deleted!',
                    text: result.message || 'Product removed from cart successfully.',
                    showConfirmButton: false,
                    timer: 1500
                });
            window.location.reload()
            } else {
                Swal.fire({
                    icon: 'error',
                    title: 'Error!',
                    text: result.error || 'Failed to remove product from cart.',
                    showConfirmButton: true
                });
            }
        } else {
            console.log("Deletion canceled by the user.");
        }
    } catch (error) {
        console.error("Unexpected error:", error);
        Swal.fire({
            icon: 'error',
            title: 'Error!',
            text: 'Something went wrong. Please try again later.',
            showConfirmButton: true
        });
    }
}


    </script>
    
















    
