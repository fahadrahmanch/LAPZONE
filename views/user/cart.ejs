        <div class="cart-table-wrapper" data-aos="fade-up" data-aos-delay="0">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table_desc">
                            <div class="table_page table-responsive">
                                <table>
                                    <!-- Start Cart Table Head -->
                                    <thead>
                                        <tr>
                                            <th class="product_remove">Delete</th>
                                            <th class="product_thumb">Image</th>
                                            <th class="product_name">Product</th>
                                            <th class="product-price">Price</th>
                                            <th class="product_quantity">Quantity</th>
                                            <th class="product_total">Total</th>
                                        </tr>
                                    </thead> <!-- End Cart Table Head -->
                                    <tbody id="cart-items">
                                        <!-- Start Cart Single Item-->
                                         <%products.forEach(item=>{%>
                                        <tr>
                                            <td class="product_remove"><a href="" class="deleteButton" onclick="dltCartItem('<%=item.productId._id %>', '<%= item.variant._id %>')"><i class="fa fa-trash-o"></i></a>
                                            </td>
                                            <td class="product_thumb"><a href=""><img
                                                        src="/uploads/<%=item.productId.productImage[0]%>"
                                                        alt=""></a></td>
                                            <td class="product_name"><a href="product-details-default.html" ><%=item.productId.productName%></a></td>
                                            <td class="product-price"> ₹ <%=item.variant.offerPrice || item.variant.salePrice%></td>
                                            <td class="product_quantity"><label></label> <input min="1"
                                                    max="5" value="<%=item.quantity%>" type="number"    onchange="updateQuantity('<%= item.productId?._id %>','<%= item.variant._id %>', this.value)"></td>
                                                    <td class="product_total" 
                                                    data-id="<%=item.productId._id%>"
                                                    data-offerPrice="<%= item.variant.offerPrice || 0 %>"
                                                    data-salePrice="<%= item.variant.salePrice || 0 %>">
                                                    ₹ <%= item.quantity * (item.variant.offerPrice || item.variant.salePrice) %>
                                                </td>
                                                                                        </tr> <!-- End Cart Single Item-->
                                        <!-- Start Cart Single Item-->
                                       <%})%>
                                    </tbody>
                                </table>
                            </div>
                            <div class="cart_submit">
                                <!-- <button class="btn btn-md btn-golden" type="submit">update cart</button> -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div> <!-- End Cart Table -->

