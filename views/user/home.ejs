<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    async function addToCart(product, qty, productDetails) {
        try {
            const productData = JSON.parse(productDetails);
            const response = await fetch('/addToCart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    product: product,
                    variant: productData.variants[0]._id,
                    qty: qty,
                })
            });

            if (response.ok) {
                const result = await response.json();
                console.log('Item added to cart:', result);

             
                Swal.fire({
                    icon: 'success',
                    title: 'Success!',
                    text: result.message || 'Item successfully added to the cart!',
                });
            } else {
                const error = await response.json();
                console.error('Error adding item:', error);

          
                Swal.fire({
                    icon: 'error',
                    title: 'Error!',
                    text: error.message || 'Failed to add item to the cart. Please try again.',
                });
            }
        } catch (err) {
            console.error('Network error:', err);

          
            Swal.fire({
                icon: 'error',
                title: 'Network Error',
                text: 'An error occurred. Please check your connection and try again.',
            });
        }
    }
</script>




